{
  "name": "infolis-web",
  "version": "0.0.5",
  "description": "Infolis Web services",
  "scripts": {
    "test": "coffee test/*.coffee | tap-spec",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "client": "browserify -t coffeeify -o public/client.js src-client/index.coffee",
    "demos": "coffee -wc -o public/ src-client/demo*.coffee",
    "watch-client": "nodemon -w src-client -e 'coffee' --exec 'npm run client'",
    "jekyll": "cd ../infolis.github.io; jekyll serve --config _config.yml,_config-dev.yml",
    "start-dev": "supervisor -w src/,data/infolis.tson,../jsonld-rapper/src,../express-jsonld/src,../mongoose-jsonld/src src/server.coffee",
    "start": "supervisor coffee src/server.coffee",
    "mongo": "mongod --dbpath data/mongodb --port 27018",
    "lint": "coffeelint -f dist/coffeelint.json src src-client",
    "run": "npm run mongo & npm run serve",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infolis/infolis-web"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infolis/infolis-web/issues"
  },
  "homepage": "https://github.com/infolis/infolis-web",
  "dependencies": {
    "accepts": "^1.3.1",
    "async": "^1.5.2",
    "body-parser": "^1.15.0",
    "coffeeify": "^1.2.0",
    "cors": "^2.7.1",
    "easylog": "^0.0.10",
    "express": "^4.13.4",
    "express-jsonld": "0.0.19",
    "jade": "^1.11.0",
    "json-stringify-safe": "^5.0.1",
    "jstransformer-coffee-script": "^1.0.0",
    "jstransformer-markdown": "^1.1.0",
    "merge": "^1.2.0",
    "mongoose": "^4.3.0",
    "mongoose-jsonld": "^0.2.4",
    "morgan": "^1.6.1",
    "multiparty": "^4.1.1",
    "qs": "^6.2.0",
    "superagent": "^1.7.2",
    "supervisor": "^0.9.1",
    "tson": "^0.0.10",
    "coffee-script": "^1.10.0",
    "vim2html": "^0.0.8",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "browserify": "^12.0.1"
  },
  "devDependencies": {
    "docker": "^0.2.14",
    "tap-spec": "^4.1.1",
    "tape": "^4.0.0"
  },
  "main": "lib/index.js"
}
